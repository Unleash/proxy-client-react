    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/react": "^18.2.27",
    "@vitest/coverage-istanbul": "^0.34.6",
    "happy-dom": "^12.9.1",
    "jsdom": "^22.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "typescript": "^5.2.2",
    "unleash-proxy-client": "^3.0.0",
    "vite": "^4.4.11",
    "vite-plugin-dts": "^3.6.0",
